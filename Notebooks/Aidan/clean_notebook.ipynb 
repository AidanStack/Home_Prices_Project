     "execution_count": 5,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "def waterfront_cleanup(df, column_name):\n",
    "    df['on_the_water'] = df[column_name]\n",
    "    df.loc[ df['on_the_water'] != 1, ['on_the_water']] = 0\n",
    "    df.loc[ df['on_the_water'] != 0, ['on_the_water']] = 1\n",
    "\n",
    "waterfront_cleanup(data_train, 'waterfront')\n",
    "data_train.on_the_water.value_counts()\n"
   ]
  },
  {
